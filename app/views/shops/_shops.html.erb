<% if shops.present? %>
			<table>
					<thead>
						<tr>
							<th>&nbsp;</th>
							<th>&nbsp;</th>
							<th>&nbsp;</th>
							<th>&nbsp;</th>
							<th>&nbsp;</th>
							<th>&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<% Array(shops).each do |shop| %>
						 <%# unless shop.name.nil? %>
							<tr>
								<td>
									<% if shop.shop_image? %>
										<%=link_to image_tag(shop.shop_image.to_s, :id => 'shops_image'), shop_path(shop) %>
									<% else%>
										<%=link_to image_tag('C0FEE.png', :id => 'no_shops_image'), shop_path(shop) %>
									<% end %>
								</td>
								<td><%= link_to shop.name, shop.url, target: :_blank, rel: :noopener %></td>
								<td><%= shop.address %></td>
									<% if user_signed_in? && @liked_shops.nil?%>
										<td id="shop_like_button_<%= shop.id %>"><%= render 'shop_likes/like_shop_button', shop: shop %></td>
										<td><%= render 'beans/register_bean_button', shop: shop %></td>
										<td><%= render 'shops/edit_shop_link', shop: shop %></td>
									<% else %>
										<td class="red-text"><i class="material-icons">favorite</i><%= shop_liked_counts(shop) %></td>
									<% end %>
							</tr>
						 <% end %>
						<%# end %>
					</tbody>
				</table>
	<% end %>
