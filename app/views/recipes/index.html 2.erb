<div class="recipes">
	<div id="search_form"><%= render 'search_form'%></div>
	<h4 id="index_title"><%= t('.title')%></h4>
	<%= render 'recipes/recipes', recipes: @recipes%>
	<%= link_to beans_path,class: 'register_recipe',class: "btn-small waves-effect waves-light tooltipped red",data:{position:"top", tooltip: t('.make_recipe')} do %>
			<%= content_tag :i, "add", class: "material-icons" %>
			<%= content_tag :i, "import_contacts", class: "material-icons" %>
	<% end %>
</div>
